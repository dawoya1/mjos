{
  "name": "mjos",
  "version": "2.1.11",
  "description": "Magic Sword Studio Operating System - Perfect Enterprise-Grade AI Operating System with 96 tests 100% passed, 0 resource leaks, production-ready",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dawoya1/mjos.git"
  },
  "homepage": "https://github.com/dawoya1/mjos#readme",
  "bugs": {
    "url": "https://github.com/dawoya1/mjos/issues"
  },
  "keywords": [
    "ai",
    "enterprise-grade",
    "production-ready",
    "zero-resource-leaks",
    "100-percent-tested",
    "team-collaboration",
    "mjos",
    "magic-sword-studio",
    "prompt-framework",
    "artificial-intelligence",
    "mcp",
    "multi-role",
    "workflow",
    "state-management",
    "memory-continuity",
    "typescript",
    "framework",
    "memory-system",
    "task-management",
    "performance-monitoring",
    "enterprise",
    "quality-assured",
    "perfect-resource-management"
  ],
  "author": "Magic Sword Studio",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  },
  "files": [
    "dist",
    "bin",
    "config",
    "docs",
    "examples",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest tests/integration.test.ts",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "clean": "rimraf dist",
    "prepare": "npm run build",
    "prepublishOnly": "npm run clean && npm run build && npm test",
    "type-check": "tsc --noEmit",
    "docs:build": "typedoc src/index.ts",
    "docs:serve": "http-server docs -p 8080",
    "quality": "node quality-report.js",
    "performance": "node test-performance.js",
    "demo": "ts-node src/index.ts",
    "demo:complete": "ts-node examples/complete-system-demo.ts",
    "mcp:server": "node bin/mjos-mcp-server.js",
    "mcp:dev": "ts-node src/mcp/server.ts",
    "mcp:install": "node scripts/install-mcp-server.js",
    "mcp:portable": "node scripts/create-portable.js",
    "release:prepare": "node scripts/prepare-release.js",
    "release:github": "git add . && git commit -m \"Release v$(node -p \"require('./package.json').version\")\" && git tag v$(node -p \"require('./package.json').version\") && git push origin main && git push origin v$(node -p \"require('./package.json').version\")",
    "release:npm": "npm publish",
    "docs": "typedoc src/index.ts",
    "cli": "ts-node src/cli/index.ts",
    "mjos": "npm run cli",
    "docs:generate": "typedoc src/index.ts --out docs/api",
    "fix:quick": "ts-node scripts/quick-fix.ts",
    "health:check": "ts-node scripts/health-check.ts"
  },
  "bin": {
    "mjos": "./dist/cli/index.js",
    "mjos-mcp-server": "./bin/mjos-mcp-server.js"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.15.1",
    "bcrypt": "^5.1.1",
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "eventemitter3": "^5.0.1",
    "express": "^4.21.2",
    "helmet": "^7.2.0",
    "inquirer": "^9.3.7",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "node-cron": "^3.0.3",
    "ora": "^8.2.0",
    "rate-limiter-flexible": "^2.4.2",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "xml2js": "^0.6.2",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.23",
    "@types/inquirer": "^9.0.8",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.10.0",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^9.0.7",
    "@types/xml2js": "^0.4.14",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.54.0",
    "glob": "^11.0.3",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "rimraf": "^6.0.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  }
}
